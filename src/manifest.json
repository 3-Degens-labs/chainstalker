{
  "manifest_version": 3,
  "name": "Chainstalker",
  "version": "1.0.4",
  "description": "Show details for ethereum addresses found on page",
  "icons": {
    "16": "images/logo-beta-640.png",
    "32": "images/logo-beta-640.png",
    "48": "images/logo-beta-640.png",
    "128": "images/logo-beta-640.png"
  },
  "content_scripts": [
    {
      "js": ["content-scripts/index.ts"],
      "matches": ["https://*/*", "http://localhost/*", "http://0.0.0.0/*"]
    }
  ]
}
